<launch>
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
		<rosparam file="$(find viratsim)/params/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
		<rosparam file="$(find viratsim)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find viratsim)/params/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find viratsim)/params/global_costmap_params.yaml" command="load" /> 
		<rosparam file="$(find viratsim)/params/base_local_planner_params.yaml" command="load" />
	</node>

</launch>
